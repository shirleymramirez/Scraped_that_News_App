{{#each saved}}
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading" id="panelHead">
                <h3 class="panel-title">{{this.title}}</h3>
            </div>
            <div class="panel-body">
                <a href="{{this.link}}" id="savedLink">{{this.link}}</a>
                <form class="navbar-form navbar-right" method="POST" action="/saved/{{this._id}}?_method=PUT">
                    <button type="button" class="btn btn-default" data-toggle="modal" data-target="#myModal{{this._id}}" id="addNote">Article Notes</button>
                    <button type="submit" class="btn btn-default" id="articleButton">Delete From Save</button>
                </form>
            </div>
        </div>
    </div>
 {{/each}}

